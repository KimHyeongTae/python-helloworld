{
  "redirects": [{ "source": "/", "destination": "/api" }],
    "builds": [
    {
      "src": "index.py",
      "use": "@now/python",
      "config": { "maxLambdaSize": "10mb" },
      "buildCommand": "pip install google-api-python-client -t ./"
    }
  ],
  "routes": [
    { "src": "/.*", "dest": "index.py" }
  ]
}
